   RowBox[{
    RowBox[{
     RowBox[{"IndicesUp", "::", "usage"}], " ", "=", " ", 
     "\"\<IndicesUp[expr] raises the indices of all the tensors of 'expr' \
using the ambient metric.\>\""}], ";"}], "\[IndentingNewLine]", 
